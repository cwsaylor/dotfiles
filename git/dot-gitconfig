[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Chris Saylor
	email = cwsaylor@gmail.com
[init]
	defaultBranch = main
[color]
	ui = true
[alias]
	s = status
	st = status
	c = commit
	sw = switch
	br = branch
	co = checkout
	dc = diff --cached
	d = diff
